{
  "Routes": [
    {
      "DownstreamPathTemplate": "/image/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "file_service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/file/image/{id}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/image/",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "file_service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/file/image/",
      "UpstreamHttpMethod": [ "POST" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}