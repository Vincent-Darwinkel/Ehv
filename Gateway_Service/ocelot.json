{
  "Routes": [
    {
      "DownstreamPathTemplate": "/directory",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "file_service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/file/directory",
      "UpstreamHttpMethod": [ "GET", "POST", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/directory/info",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "file_service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/file/directory/info",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/file",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "file_service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/file/file",
      "UpstreamHttpMethod": [ "GET", "POST", "DELETE" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}